graph TD
    subgraph Root["/"]
        A[package.json] --> B[tailwind.config.js]
        A --> C[postcss.config.js]
        
        subgraph src["/src"]
            D[App.js] --> E[components/]
            D --> F[pages/]
            D --> G[styles/]
        end
    end

    subgraph Components["/components"]
        H[Navigation.js]
        I[Footer.js]
        J[Forms/]
    end

    subgraph Pages["/pages"]
        K[HomePage.js]
        L[IdTirageUnique.js]
        M[IdTirageMultiple.js]
