  <div className="container mx-auto p-6">
    <h1 className="text-2xl font-bold text-white mb-6">ID-Tirage Multiple</h1>
    {/* Contenu du formulaire multiple */}
  </div>
);

// Composant Tableau
const TableauResultats = () => (
  <div className="container mx-auto p-6">
